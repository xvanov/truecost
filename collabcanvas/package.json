{
  "name": "collabcanvas",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:production": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "cross-env SKIP_AI_TESTS=true vitest run",
    "test:ai": "vitest run src/services/aiService*.test.ts",
    "test:perf": "playwright test --config test/perf/playwright.perf.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "deploy": "npm run build:production && firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules,database:rules"
  },
  "dependencies": {
    "@capacitor-firebase/authentication": "^7.5.0",
    "@capacitor/android": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "firebase": "^12.4.0",
    "jspdf": "^3.0.3",
    "konva": "^10.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-konva": "^19.0.10",
    "react-pdf": "^10.2.0",
    "react-router-dom": "^7.9.5",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.0.0",
    "@eslint/js": "^9.36.0",
    "@faker-js/faker": "^9.9.0",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@playwright/test": "^1.50.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/xlsx": "^0.0.35",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "chromium": "^3.0.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "firebase-tools": "^14.19.1",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
